  publisher     = {Privacy Enhancing Technologies Symposium Advisory Board},
  title         = {The effectiveness of adaptation methods in improving user
engagement and privacy protection on social network sites},
  volume        = {2022},
  year          = {2022}
}

@article{NarrativesOfThe2022,
