@article{Starbird-2019-DisinformationCollaborativeOperations-x,
  _source_file  = {my_refs/references (2).bib},
  abstract      = {In this paper, we argue that strategic information operations
(e.g. disinformation, political propaganda, and other forms of
online manipulation) are a critical concern for CSCW researchers,
