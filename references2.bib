  doi           = {10.1145/3544548.3581258},
  isbn          = {9781450394215},
  keywords      = {deceptive design, community safety, anxiety, manipulative design,
dark infrastructure, dark patterns, fear, safety technologies,
crime, safety},
  location      = {<conf-loc>, <city>Hamburg</city>, <country>Germany</country>, </conf-loc>},
  month         = {April},
  number        = {Article 193},
  pages         = {1--18},
