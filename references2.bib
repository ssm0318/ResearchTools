@article{ConnectionSocial2011,
  _source_file  = {my_refs/references (5).bib},
  abstract      = {This study assesses whether Facebook users have different
‘connection strategies,’ a term which describes a suite of
Facebook-related relational communication activities, and
explores the relationship between these connection strategies and
social capital. Survey data ( N = 450) from a random sample of
undergraduate students reveal that only social
