  volume        = {592},
  year          = {2004}
}

@article{MeasuringThePrevalence2022,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {With increasing attention to online anti-social behaviors such as
personal attacks and bigotry, it is critical to have an accurate
accounting of how widespread anti-social behaviors are. In this
paper, we empirically measure the prevalence of anti-social
behavior in one of the world's most popular online community
platforms. We operationalize this goal as measuring the proportion
of unmoderated comments in the 97 most popular communities on
Reddit that violate eight widely accepted platform norms. To
achieve this goal, we contribute a human-AI pipeline for
identifying these violations and a bootstrap sampling method to
quantify measurement uncertainty. We find that 6.25\% (95\%
Confidence Interval [5.36\%, 7.13\%]) of all comments in 2016, and
4.28\% (95\% CI [2.50\%, 6.26\%]) in 2020, are violations of these
norms. Most anti-social behaviors remain unmoderated: moderators
only removed one in twenty violating comments in 2016, and one in
ten violating comments in 2020. Personal attacks were the most
prevalent category of norm violation; pornography and bigotry were
the most likely to be moderated, while politically inflammatory
comments and misogyny/vulgarity were the least likely to be
moderated. This paper offers a method and set of empirical results
for tracking these phenomena as both the social practices (e.g.,
moderation) and technical practices (e.g., design) evolve.},
  author        = {Park, Joon Sung and Seering, Joseph and Bernstein, Michael S},
  doi           = {10.1145/3555552},
  issn          = {2573-0142},
  journal       = {Proc. ACM Hum.-Comput. Interact.},
  language      = {en},
  month         = {November},
  number        = {CSCW2},
  pages         = {1--29},
  title         = {Measuring the Prevalence of {Anti-Social} Behavior in Online Communities},
  volume        = {6},
  year          = {2022}
}

