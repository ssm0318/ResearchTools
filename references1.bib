response, reproductive health, support provision, stigma,
pregnancy loss, social support},
  month         = {December},
  number        = {6},
  pages         = {1--29},
  publisher     = {Association for Computing Machinery},
  title         = {Responding to Sensitive Disclosures on Social Media: A {Decision-Making} Framework},
  volume        = {25},
  year          = {2018}
}

