  keywords      = {Privacy; Social network sites; Communication Privacy Management
theory (CPM); Facebook; Disclosure},
  month         = {June},
  pages         = {444--454},
  title         = {Managing privacy boundaries together: Exploring individual and
group privacy management strategies in Facebook},
  volume        = {35},
  year          = {2014}
}

