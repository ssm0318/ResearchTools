@article{PeerVictimizationAnd2023,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {Peer victimization is a serious risk factor for the development of
problematic social media use (PSMU) among adolescents. However,
little is known about the mediating and moderating mechanisms
underlying this association. The current study tested whether
psychological insecurity mediated the relation between peer
victimization and PSMU, and whether this mediating process was
