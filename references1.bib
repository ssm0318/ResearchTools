  issn          = {1096-4037,1573-2827},
  journal       = {Clin. Child Fam. Psychol. Rev.},
  keywords      = {Adolescents; Friendship; Peer relations; Relationship quality;
Review; Social media},
  language      = {en},
  month         = {September},
  number        = {3},
  pages         = {267--294},
  pmc           = {PMC6435354},
  pmid          = {29627907},
  title         = {Transformation of Adolescent Peer Relations in the Social Media Context: Part 2-Application to Peer Group Processes and Future Directions for Research},
  volume        = {21},
  year          = {2018}
}

