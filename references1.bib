  location      = {Vancouver, BC, Canada},
  month         = {May},
  pages         = {3217--3226},
  publisher     = {Association for Computing Machinery},
  series        = {CHI '11},
  title         = {We're in it together: interpersonal management of disclosure in
social network services},
  year          = {2011}
