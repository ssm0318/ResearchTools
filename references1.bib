  language      = {en},
  month         = {March},
  shorttitle    = {Feeling authentic on social media},
  title         = {Feeling authentic on social media: {Subjective} authenticity across {Instagram} {Stories} and {Posts}},
  type          = {preprint},
  url           = {https://osf.io/jz3wm},
  urldate       = {2022-12-02},
