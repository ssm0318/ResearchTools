  issn          = {2573-0142},
  journal       = {Proc. ACM Hum.-Comput. Interact.},
  language      = {en},
  month         = {April},
  number        = {CSCW1},
  pages         = {1--32},
  title         = {Does This Photo Make Me Look Good?: How Posters, Outsiders, and
Friends Evaluate Social Media Photo Posts},
  volume        = {5},
  year          = {2021}
}

