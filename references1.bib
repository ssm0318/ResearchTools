@article{CommunicationEffects2002,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {This investigation examined how computer-mediated communication
(CMC) partners exchange personal information in initial
interactions, focusing on the effects of communication channels
on self-disclosure, question-asking, and uncertainty reduction.
Unacquainted individuals (N = 158) met either face-to-face or via
CMC. Computer-mediated interactants exhibited a greater
proportion of more direct and intimate uncertainty reduction
