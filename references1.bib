@inproceedings{KeepingInTouch2008,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {Many observers have praised new communication technologies for
providing convenient and affordable tools for maintaining
relationships at a distance. Yet the precise role of mediated
communication in relationship maintenance has been difficult to
isolate. In this paper, we treat residential moves as natural
