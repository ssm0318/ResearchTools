@inproceedings{UnderstandingTheDigital2022,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {We collected Instagram Direct Messages (DMs) from 100 adolescents
and young adults (ages 13-21) who then flagged their own
conversations as safe or unsafe. We performed a mixed-method
analysis of the media files shared privately in these
conversations to gain human-centered insights into the risky
