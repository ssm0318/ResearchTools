}

@article{FindingAntecedentsOf2015,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {PURPOSE: The study's aim is to review the literature on
psychological safety in the healthcare setting, more specifically,
to identify environmental climates which promote and support
psychological safety in healthcare organizations. CONCLUSION: The
findings show the complex dyadic interplay between leaders and
