  isbn          = {9781461386063},
  journal       = {The social ecology of crime},
  pages         = {47--62},
  publisher     = {Springer New York},
  title         = {Fear and Its Relationship to Crime, Neighborhood Deterioration,
and Informal Social Control},
  year          = {1986}
}

