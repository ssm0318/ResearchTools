@inproceedings{SocialCapitalOn2011,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {Though social network site use is often treated as a monolithic
activity, in which all time is equally social and its impact the
same for all users, we examine how Facebook affects social
capital depending upon: (1) types of site activities, contrasting
one-on-one communication, broadcasts to wider audiences, and
