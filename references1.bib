  number        = {5},
  pages         = {841--855},
  shorttitle    = {Attachment, authenticity, and honesty},
  title         = {Attachment, authenticity, and honesty: {Dispositional} and experimentally induced security can reduce self- and other-deception.},
  url           = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0019206},
  urldate       = {2022-11-22},
