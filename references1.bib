@article{TowardATheory2004,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {The relationship between uncertainty and information has long
been at the forefront of the social scientific study of human
behavior. The last decade has seen increased attention among
communication scholars to the information-management process. The
result has been significant widening of ideological lenses and an
impressive growth of knowledge. However, a review of the
