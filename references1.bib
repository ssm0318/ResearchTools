@inproceedings{ItTakesA2020,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {While the majority of research in chatbot design has focused on
creating chatbots that engage with users one-on-one, less work
has focused on the design of conversational agents for online
communities. In this paper we present results from a three week
