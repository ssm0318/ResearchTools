@article{YourFinsta2023,
  _source_file  = {my_refs/references (10).bib},
  abstract      = {This study explores emerging adults? reflections about how their
secondary Instagram account (?Finsta? or Fake Instagram)
self-presentation evolved over time, from account creation to
their present use. Drawing on interview data collected from
female emerging adults (N = 17) who had at least one Finsta and
