  isbn          = {9781450349819},
  keywords      = {eye tracking, saliency, computer vision, machine learning,
graphic design, visualization, deep learning, retargeting},
  location      = {Qu{\'e}bec City, QC, Canada},
  month         = {October},
  pages         = {57--69},
  publisher     = {Association for Computing Machinery},
  series        = {UIST '17},
  title         = {Learning Visual Importance for Graphic Designs and Data
