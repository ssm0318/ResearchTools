Systems},
  doi           = {10.1145/3613904.3642543},
  isbn          = {9798400703300},
  keywords      = {aesthetic experience, autoethnography, player experience,
reflection, transformation, transformative experience, video
games},
  month         = {May},
  number        = {Article 808},
  pages         = {1--20},
  publisher     = {Association for Computing Machinery},
  series        = {CHI '24},
  title         = {From Disorientation to Harmony: Autoethnographic Insights into
Transformative Videogame Experiences},
  year          = {2024}
}

@inproceedings{walsh2010,
  _source_file  = {my_refs/references (1).bib},
  abstract      = {As technology for children becomes more mobile, social, and
