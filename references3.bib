}

@inproceedings{yu2017,
  _source_file  = {my_refs/references (1).bib},
  abstract      = {Productive and dedicated members are critical to the success of
online production communities like Wikipedia. Many communities
organize in subgroups where members voluntarily work on projects
of shared interest. In this paper, we investigate how members'
pre-joining connections with the subgroup predict their
productivity and withdrawal after joining. Drawing insights from
