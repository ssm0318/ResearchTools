@article{martinez2022,
  _source_file  = {my_refs/references (1).bib},
  abstract      = {The video game Animal Crossing: New Horizons (AC:NH) launched
during the COVID-19 pandemic and families turned to it as a game
to play together during isolation. This interview study of 27
families considered how families used AC:NH for Joint Media
Engagement (JME), where family members engage with media content
