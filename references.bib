@article{WeickWeick-1988-EnactedSensemakingSituations1-f,
 abstract = {ABSTRACTSensemaking in crisis conditions is made more difficult
because action that is instrumental to understanding the crisis
often intensifies the crisis. This dilemma is interpreted from
the perspective that people enact the environments which
constrain them. It is argued that commitment, capacity, and
expectations affect sensemaking during crisis and the severity of
the crisis itself. It is proposed that the core concepts of
enactment may comprise an ideology that reduces the likelihood of
crisis.},
 author = {Weick, Karl E},
 doi = {10.1111/j.1467-6486.1988.tb00039.x},
 issn = {0022-2380,1467-6486},
