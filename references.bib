@article{UzziRivera-2010-DynamicsDyadsMechanisms-c,
 abstract = {Embeddedness in social networks is increasingly seen as a root
cause of human achievement, social stratification, and actor
behavior. In this article, we review sociological research that
examines the processes through which dyadic ties form, persist,
and dissolve. Three sociological mechanisms are overviewed:
assortative mechanisms that draw attention to the role of actors'
attributes, relational mechanisms that emphasize the influence of
existing relationships and network positions, and proximity
mechanisms that focus on the social organization of interaction.},
 author = {Rivera, Mark T and Soderstrom, Sara B and Uzzi, Brian},
 doi = {10.1146/annurev.soc.34.040507.134743},
 issn = {0360-0572,1545-2115},
