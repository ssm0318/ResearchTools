 issn = {0022-006x},
 journal = {Journal of consulting and clinical psychology},
 keywords = {Adolescent ; Adolescent Development ; Adolescents ; Adult ; Adults ; Alcohol Drinking - epidemiology ; Alcohol Use ; Alcoholism ; Binge Drinking ; Child ; Clinical psychology ; Drinking ; Drinking Behavior ; Drinking of alcoholic beverages ; Drug abuse ; Drug Usage ; Emerging Adulthood ; Female ; Follow-Up Studies ; Forecasting ; Human ; Humans ; Male ; Psychological aspects ; Psychology ; Risk Factors ; Substance abuse ; Substance-Related Disorders - epidemiology ; Teenagers},
 language = {eng},
