@article{ThomaesSedikides-2017-StateAuthenticity-m,
 abstract = {State authenticity is the sense that one is currently in alignment
with one’s true or real self. We discuss state authenticity as
seen by independent raters, describe its phenomenology, outline
its triggers, consider its well-being and behavioral implications,
and sketch out a cross-disciplinary research agenda.},
