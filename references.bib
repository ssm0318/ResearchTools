 author = {Hiniker, Alexis and Sobel, Kiley and Lee, Bongshin},
 booktitle = {Proceedings of the 2017 CHI Conference on Human Factors in
Computing Systems},
 doi = {10.1145/3025453.3025588},
 isbn = {9781450346559},
 keywords = {comicboarding, design workshop, early childhood, fictional
inquiry, participatory design},
 month = {May},
 pages = {5767--5772},
 publisher = {Association for Computing Machinery},
