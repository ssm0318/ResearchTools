@article{HanLee-2014-SocialMediaStates-e,
 abstract = {This study examined the relationships among social media use for
information, self-status seeking and socializing, body image,
self-esteem, and psychological well-being, and some cultural
effects moderating these relationships. Americans (n = 502) and
Koreans (n = 518) completed an online survey. The main findings
showed that (a) social media use for information about body image
is negatively related to body satisfaction in the United States
and Korea, while social media use for self-status seeking
regarding body image is positively related to body satisfaction
only in Korea; and (b) body satisfaction has direct and indirect
positive effects on psychological well-being manifested in
similar ways in the United States and Korea. Implications and
future research directions are discussed.},
 author = {Lee, Hye-Ryeon and Lee, Hye Eun and Choi, Jounghwa and Kim, Jang
Hyun and Han, Hae Lin},
