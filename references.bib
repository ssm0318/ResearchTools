 issn = {2474-9567},
 journal = {Proceedings of ACM on interactive, mobile, wearable and ubiquitous technologies},
 keywords = {biosignals ; Communication ; Computer Science - Human-Computer Interaction ; Heart rate ; interpersonal communication ; Lightweight ; Smartwatches ; Weight reduction},
 language = {eng},
 number = {1},
