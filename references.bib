@article{VostroknutovGross-2022-WhyPeople-q,
 abstract = {Norms prescribe how to make decisions in social situations and
play a crucial role in sustaining cooperative relationships and
coordinating collective action. However, following norms often
requires restricting behavior, demanding to curtail selfishness,
or suppressing personal goals. This raises the question why people
adhere to norms. We review recent theories and empirical findings
that aim at explaining why people follow norms even in private,
when violations are difficult to detect and are not sanctioned. We
discuss theories of norm internalization, social and self-image
concerns, and social learning (i.e. preferences conditional on
what others do/believe). Finally, we present two behavioral,
incentivized tasks that can be used to elicit norms and measure
the individual propensity to follow them.},
 author = {Gross, Jörg and Vostroknutov, Alexander},
 doi = {10.1016/j.copsyc.2021.08.016},
 issn = {2352-2518,2352-250X},
 journal = {Curr Opin Psychol},
 keywords = {Conditional preferences; Cooperation; Norms; Rule following;
Self-image; Social image; Social learning},
 language = {en},
 month = {April},
 pages = {1--6},
 pmid = {34520935},
 title = {Why do people follow social norms?},
 volume = {44},
 year = {2022}
}

