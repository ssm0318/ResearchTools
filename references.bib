@book{OfficeOfTheSurgeonGeneralOSGOfficeOfTheSurgeonGeneralOSG-2023-EpidemicLonelinessCommunity-b,
 abstract = {Our relationships and interactions with family, friends,
colleagues, and neighbors are just some of what create social
connection. Our connection with others and our community is also
informed by our neighborhoods, digital environments, schools, and
workplaces. Social connection— the structure, function, and
