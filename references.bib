@article{HodgesPeets-2018-AuthenticityFriendshipsAdolescence-u,
 abstract = {Only a handful of studies have focused on understanding how
authenticity in close relationships may be related to individuals’
wellbeing. In this study we examined whether authenticity in a
friendship was related to greater adjustment during adolescence.
Participants were 318 sixth- and ninth-grade students (155 boys;
Mage 5 13.46, SDage 5 1.51). Our results show that adolescents who
feel more authentic have more positive self-views, are less lonely
at school, and more satisfied with their relationship.
Importantly, authenticity–adjustment associations remained
significant after controlling for positive friendship quality and
conflict. These findings suggest that authenticity should be more
often incorporated into the study of different friendship
processes.},
 author = {Peets, Kätlin and Hodges, Ernest V E},
 doi = {10.1111/sode.12254},
 issn = {0961-205X},
 journal = {Soc. Dev.},
 language = {en},
 number = {1},
 pages = {140--153},
 title = {Authenticity in friendships and well-being in adolescence},
 volume = {27},
 year = {2018}
}

