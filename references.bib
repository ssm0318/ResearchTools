@article{Kondakciu2021-ik,
 abstract = {Purpose In response to calls for more research on gender(s) in digital contexts, this paper aims to ask, how do individuals engage in self-presentation of their gender identities on social media? Design/methodology/approach Using a multi-method qualitative approach, this research explores the narratives of 17 Millennials as they negotiate their online gender expressions with a particular focus on the image-based social platforms, Facebook and Instagram. Specifically, in-depth interviews, a collage technique and visual data from informants' social media pages were analyzed to identify emergent themes. Findings Drawing on the theoretical work of Goffman's (1971) self-presentation and Butler's (1999) gender performance, this research highlights a pervading discourse of authenticity or the desire for Millennial social media users to craft and perform a perceived ``authentic self'' online. This often entails both expressions of gender fluidly and gender policing. Further, four strategies emerge in the data which reveal how individuals negotiate and navigate their gendered self-presentation online, either in an agentic manner or as a protective measure. Originality/value While much research exists on online self-presentation, gender(s) has been under-researched in a digital context. Existing studies examine the content of social media pages (e.g. Facebook profiles or women's Instagram pages) as it relates to gender, but largely do not explore the lived experiences and narratives of individuals as they negotiate their gendered expressions. In addition, the use of visual data through the collage technique adds valuable insight into how gender is experienced and performed. Findings reveal that while Millennials are often touted as a gender-fluid generation, tensions still exist in online gendered expressions.},
 author = {Kondakciu, Klaudia and Souto, Melissa and Zayer, Linda Tuncay},
 journal = {Qualitative Market Research: An International Journal},
 month = {January},
 number = {1},
 pages = {80--99},
 title = {Self-presentation and gender on social media: an exploration of the expression of ``authentic selves''},
 volume = {25},
 year = {2021}
}

@article{KonstanHarper-2015-MovieLensDatasetsContext-l,
 abstract = {The MovieLens datasets are widely used in education, research,
and industry. They are downloaded hundreds of thousands of times
each year, reflecting their use in popular press programming
books, traditional and online courses, and software. These
datasets are a product of member activity in the MovieLens movie
