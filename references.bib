 pages = {1665--1680},
 publisher = {Association for Computing Machinery},
 series = {Ccs '20},
 title = {{Impersonation-as-a-Service}: Characterizing the Emerging Criminal Infrastructure for User Impersonation at Scale},
 year = {2020}
}

@inproceedings{CamposCaraban-2019-23WaysInteraction-m,
 abstract = {Ten years ago, Thaler and Sunstein introduced the notion of
nudging to talk about how subtle changes in the 'choice
architecture' can alter people's behaviors in predictable ways.
This idea was eagerly adopted in HCI and applied in multiple
contexts, including health, sustainability and privacy. Despite
this, we still lack an understanding of how to design effective
technology-mediated nudges. In this paper we present a systematic
review of the use of nudging in HCI research with the goal of
laying out the design space of technology-mediated nudging - the
why (i.e., which cognitive biases do nudges combat) and the how
(i.e., what exact mechanisms do nudges employ to incur behavior
change). All in all, we found 23 distinct mechanisms of nudging,
grouped in 6 categories, and leveraging 15 different cognitive
biases. We present these as a framework for technology-mediated
nudging, and discuss the factors shaping nudges' effectiveness
and their ethical implications.},
 address = {New York, NY, USA},
 author = {Caraban, Ana and Karapanos, Evangelos and Gonçalves, Daniel and
Campos, Pedro},
 booktitle = {Proceedings of the 2019 CHI Conference on Human Factors in
Computing Systems},
 doi = {10.1145/3290605.3300733},
 isbn = {9781450359702},
 keywords = {persuasive technology, behavioral economics, nudging},
 month = {May},
 number = {Paper 503},
 pages = {1--15},
 publisher = {Association for Computing Machinery},
 series = {CHI '19},
 title = {23 Ways to Nudge: A Review of Technology-Mediated Nudging in
Human-Computer Interaction},
 year = {2019}
}

