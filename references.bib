@inproceedings{Axtell2022-it,
 abstract = {Interactions with our personal and family pictures are essential to continued social reminiscence, leading to long-term benefts, including reduced social isolation. Previous research has identifed how designs of digital picture tools fall short of physical options specifcally in terms of reminiscence. However, the relative prompting abilities of diferent digital interactions, including the types of memories prompted like external facts or person-centred memories, have not yet been explored. To investigate this, we present a controlled study of the memories prompted by three digital picture interactions (slideshow, gallery, and tabletop) on personal touchscreen devices. We fnd diferences in how these tools and the interactions they support prompt reminiscence. In particular, gallery views prompt signifcantly fewer memories than either the tabletop or slideshow. Slideshows prompt signifcantly more external, factual memories, but not more person-centred memories, which are key to reminiscence. This has implications for the overall social usability of digital picture interactions.},
 address = {New Orleans LA USA},
 author = {Axtell, Benett and Saryazdi, Raheleh and Munteanu, Cosmin},
 booktitle = {{CHI} Conference on Human Factors in Computing Systems},
 conference = {CHI '22: CHI Conference on Human Factors in Computing Systems},
 language = {en},
 month = {April},
 pages = {1--12},
 publisher = {Acm},
 title = {Design is Worth a Thousand Words: The Effect of Digital Interaction Design on {Picture-Prompted} Reminiscence},
 year = {2022}
}

