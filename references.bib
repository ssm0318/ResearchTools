@article{RiesmeyerZillich-2021-BeYourselfInstagram-u,
 abstract = {This article examines the relative importance of personal,
descriptive, and injunctive norms for adolescents?
self-presentation on Instagram and analyzes the role of proximal
and distal reference groups in norm negotiation. Based on 27
semi-structured interviews with German Instagram users between 14
and 19?years old, we identified four types of adolescents?
self-presentation that differ in terms of norms and referent
