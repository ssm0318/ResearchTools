@inproceedings{WilsonWisniewski-2012-FightingMyRegulation-m,
 abstract = {Sharing information online via social network sites (SNSs) is at
an all-time high, yet research shows that users often exhibit a
marked dissatisfaction in using such sites. A compelling
explanation for this dichotomy is that users are struggling
