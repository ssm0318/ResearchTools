@inproceedings{HaimsonLiu-2020-ReasonsSharingTransitions-j,
 abstract = {During life transitions, people sometimes turn to social media
audiences separate from their typical online networks. By
qualitatively analyzing open-ended data from a U.S.-based survey
(N= 775), we examined why and how people discuss life transitions
with these separate audiences. Survey questions asked about life
events experienced, separate networks and the interactions that
occurred there, and participants' reasoning behind these online
behaviors. We found that people use separate networks, especially
online support groups, to interact with others anonymously,
receive informational and emotional support, and have direct and
focused discussions with people with similar experiences.},
