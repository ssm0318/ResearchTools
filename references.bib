@article{SchynsJack-2017-TowardSocialCommunication-s,
 abstract = {As a highly social species, humans are equipped with a powerful
tool for social communication-the face. Although seemingly simple,
the human face can elicit multiple social perceptions due to the
rich variations of its movements, morphology, and complexion.
Consequently, identifying precisely what face information elicits
different social perceptions is a complex empirical challenge that
has largely remained beyond the reach of traditional methods. In
the past decade, the emerging field of social psychophysics has
developed new methods to address this challenge, with the
potential to transfer psychophysical laws of social perception to
the digital economy via avatars and social robots. At this
exciting juncture, it is timely to review these new methodological
developments. In this article, we introduce and review the
foundational methodological developments of social psychophysics,
present work done in the past decade that has advanced
understanding of the face as a tool for social communication, and
discuss the major challenges that lie ahead.},
 author = {Jack, Rachael E and Schyns, Philippe G},
 doi = {10.1146/annurev-psych-010416-044242},
 issn = {0066-4308,1545-2085},
 journal = {Annu. Rev. Psychol.},
 keywords = {culture; facial expressions; reverse correlation; social
communication; social psychophysics},
 language = {en},
 month = {January},
 pages = {269--297},
 pmid = {28051933},
 title = {Toward a Social Psychophysics of Face Communication},
 volume = {68},
 year = {2017}
}

