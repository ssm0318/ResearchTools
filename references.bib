@article{othersDuck-1988-HandbookPersonalInterventions-s,
 abstract = {This is the first and only comprehensive volume in a field that
has grown exponentially in the last ten years. The book is
organized across disciplines to reflect the nature of the field
and has a broad range of appeal to a variety of teachers and
researchers in those widely ranging disciplines. The ``Handbook''
is therefore divided into sections that reflect and describe the
inputs of the different disciplines: Communication; Community and
Clinical Psychology; Developmental and Life-cycle Psychology;
Social Psychology and Sociology. There is also a section on
Conceptualizations and Methods, including a chapter from the
tradition of research into personality. The chapters are
contributed by experts in the field and individual sections are
edited by leading authorities. The ``Handbook'' thus reflects the
view that the field of research in personal relationships is not
created nor sustained by the efforts of only one section of the
community of scholars alone, but instead feeds and grows from the
interaction of many different schools of thought, all of which
need to be informed about the work of the others. All the
chapters are forward looking and contain the seeds for the future
growth of the discipline. (PsycInfo Database Record (c) 2020 APA,
all rights reserved)},
 address = {Oxford, England},
 author = {Duck, S E and Hay, D F and Hobfoll, S E and Ickes, W E and
{others}},
 editor = {Duck, Steve and Hay, Dale F and Hobfoll, Stevan E and Ickes,
William and Montgomery, Barbara M},
 publisher = {John Wiley \& Sons Handbook of personal relationships},
 title = {Handbook of personal relationships: Theory, research and
interventions},
 volume = {702},
 year = {1988}
}

