@article{SteinbergSteinberg-2005-CognitiveAffectiveAdolescence-q,
 abstract = {Questions about the nature of normative and atypical development
in adolescence have taken on special significance in the last few
years, as scientists have begun to recast old portraits of
adolescent behavior in the light of new knowledge about brain
development. Adolescence is often a period of especially
heightened vulnerability as a consequence of potential
disjunctions between developing brain, behavioral and cognitive
systems that mature along different timetables and under the
control of both common and independent biological processes. Taken
together, these developments reinforce the emerging understanding
of adolescence as a critical or sensitive period for a
reorganization of regulatory systems, a reorganization that is
fraught with both risks and opportunities.},
 author = {Steinberg, Laurence},
 doi = {10.1016/j.tics.2004.12.005},
 issn = {1364-6613},
