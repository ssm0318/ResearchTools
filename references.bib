@inproceedings{KoDruga-2021-Children'sPerceptions-p,
 abstract = {Children are increasingly surrounded by AI technologies but can
overestimate smart devices’ abilities due to their lack of
transparency. Drawing on the sense-making theory, this study
explores how children come to see machine intelligence after
training custom machine learning models and creating smart
programs that use them. Through a 4-week observational study in
after-school programs with 52 children (7 to 12 years old), we
found that children engage in the scientific method while
training, coding and testing their smart programs. We also found
that children became more skeptical of certain abilities of smart
devices as they shifted their attribution of agency from the
devices to the people who program them. These changes in
perception happened both through individual interactions with
agents and prompted debates with peers. Based on these results,
we conclude with discussions on strategies for promoting
children’s sense-making practices and sense of agency in the age
of machine learning.},
 address = {New York, NY, USA},
 author = {Druga, Stefania and Ko, Amy J},
 booktitle = {Interaction Design and Children},
 doi = {10.1145/3459990.3460712},
 isbn = {9781450384520},
 keywords = {Children, AI literacy, Child-Agent Interaction, Machine
Intelligence},
 month = {June},
 pages = {49--61},
 publisher = {Association for Computing Machinery},
 series = {IDC '21},
 title = {How do children’s perceptions of machine intelligence change when
training and coding smart programs?},
 year = {2021}
}

@article{KohlsSauer-2011-ImplicitExplicitMindfulness-p,
 abstract = {The objective of this study was to examine whether the ‘‘step back
and watch’’ attitude of mindfulness manifests in less emotional
behavior. We hypothesized that the ‘‘acceptance’’ facet of
mindfulness, but not the ‘‘presence’’ facet, is negatively
associated with the magnitude of emotional behavior in four tests,
i.e., (1) rating of words, (2) rating of aversive and neutral
pictures, and (3) evaluative conditioning (EC). Additionally, we
hypothesized that (4) the acceptance facet is associated with
increased reaction time (RT) in an emotional Stroop test, and that
the presence facet is associated with decreased RT and lower error
rate. The sample consisted of N = 247 non-clinical adults and was
tested in a crosssectional study. The results provide partial
evidence that the acceptance facet of mindfulness may be
associated with less aversive reactions towards aversive stimuli.
Future studies should substantiate these ﬁndings but also
determine their clinical relevance.},
 author = {Sauer, Sebastian and Walach, Harald and Schmidt, Stefan and
Hinterberger, Thilo and Horan, Majella and Kohls, Niko},
 doi = {10.1016/j.concog.2011.08.002},
 issn = {1053-8100},
 journal = {Conscious. Cogn.},
 language = {en},
 number = {4},
 pages = {1558--1569},
 title = {Implicit and explicit emotional behavior and mindfulness},
 volume = {20},
 year = {2011}
}

